/*
    测试 continue 和 break
*/
int index := 0;
while(index <= 3){
    if(index > 2){
        printf("break");
        break;
    }else{


        index := index + 1;
        printf("continue");
        continue;

    }
}



for(int i:=0; i<=2; i:=i+1){
    if(i<=1){
        for(int j:=0; j< 2; j:=j+1){
            printf("Break!");
        }
    }else{
        break;
        printf("over!");
    }
}

